<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.or.matsam.manage.tag.service.impl.TagMapper">
    <insert id="insert" parameterType="kr.or.matsam.manage.tag.entity.TagVO">
        INSERT INTO RES_TAG_TB(
            RES_SERIAL_NO,
            CATE_CD,
            TAG_CONTENT
        ) VALUES (
            #{resSerialNo},
            #{cateCd},
            #{tagContent}
        )
    </insert>

    <delete id="delete" parameterType="java.lang.String">
        DELETE FROM RES_TAG_TB
        WHERE       RES_SERIAL_NO = #{resSerialNo}
    </delete>

    <select id="listAll" resultType="kr.or.matsam.manage.tag.entity.TagVO" parameterType="java.lang.String">
        SELECT  RES_SERIAL_NO,
                CATE_CD,
                TAG_CONTENT,
        FROM    res_tag_tb
        WHERE   res_serial_no = #{resSerialNo}
    </select>
</mapper>